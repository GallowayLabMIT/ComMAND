    "\n",
    "# slope\n",
    "ax = axes[2]\n",
    "for construct, group in plot_df.groupby('construct', sort=False):\n",
    "    sns.stripplot(data=group, x='construct_loc', y='slope', hue='construct', palette=main_palette,\n",
