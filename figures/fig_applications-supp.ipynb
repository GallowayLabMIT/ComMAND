    "# stat gmean\n",
    "ax = axes[0]\n",
    "for construct, group in plot_df.groupby('construct', sort=False):\n",
    "    sns.stripplot(data=group, x='construct_loc', y='output_gmean', hue='construct', palette=main_palette,\n",
