    "ax.set(title='Slope', xlabel='', ylabel='', ylim=(0,1.2))\n",
    "ax.set_xticklabels([ts_label[l.get_text()] for l in ax.get_xticklabels()], ha='right', rotation=45)\n",
    "sns.despine(ax=ax)\n",
